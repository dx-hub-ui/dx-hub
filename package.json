{
  "name": "dxhub",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "engines": { "node": ">=22 <23" },
  "scripts": {
    "dev": "turbo run dev --parallel --filter=web",
    "build": "turbo run build --filter=web",
    "lint": "turbo run lint",
